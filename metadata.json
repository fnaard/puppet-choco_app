{
  "name": "gabe-choco_app",
  "version": "0.1.0",
  "author": "Gabriel M Schuyler",
  "license": "Apache 2.0",
  "summary": "Installs the Chocolatey package manager and configures sources",
  "source": "https://github.com/fnaard/puppet-choco_app",
  "project_page": "https://github.com/fnaard/puppet-choco_app",
  "issues_url": "https://github.com/fnaard/puppet-choco_app/issues",
  "tags": ["windows","chocolatey","choco","package manager"],
  "dependencies": [
    {"name":"puppetlabs-concat","version_requirement":">= 1.0.0"},
    {"name":"puppetlabs-powershell","version_requirement":">= 1.0.0"},
    {"name":"puppetlabs-stdlib","version_requirement":">= 1.0.0"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Windows",
      "operatingsystemrelease": [
        "Server 2003",
        "Server 2003 R2",
        "Server 2008",
        "Server 2008 R2",
        "Server 2012",
        "Server 2012 R2",
        "7",
        "8",
        "10"
      ]
    }
  ]
}

